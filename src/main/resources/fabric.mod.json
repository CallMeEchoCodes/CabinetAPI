{
    "schemaVersion": 1,
    "id": "cabinetapi",
    "version": "${version}",
    "name": "Cabinet API",
    "description": "A library mod with a bunch of useful utilities for modders.",
    "authors": [
        "CallMeEcho"
    ],
    "contact": {
        "homepage": "https://github.com/CallMeEchoCodes/CabinetAPI",
        "sources": "https://github.com/CallMeEchoCodes/CabinetAPI",
        "issues": "https://github.com/CallMeEchoCodes/CabinetAPI/issues"
    },
    "license": "MPL-2.0",
    "environment": "*",
    "entrypoints": {
        "main": [
            "dev.callmeecho.cabinetapi.CabinetAPI"
        ],
        "client": [
            "dev.callmeecho.cabinetapi.client.CabinetAPIClient"
        ]
    },
    "mixins": [
        "cabinetapi.mixins.json"
    ],
    "icon": "assets/cabinetapi/icon.png",
    "accessWidener": "cabinetapi.accesswidener",
    "depends": {
        "fabricloader": ">=0.14.24",
        "minecraft": "${minecraft_version}",
        "java": ">=17",
        "fabric-api": "*"
    },
    "custom": {
        "modmenu": {
            "links": {
                "modmenu.discord": "https://discord.gg/TTmx7d2axf"
            },
            "badges": [ "library" ]
        }
    }
}
